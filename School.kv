<FirstPage>:
    orientation:"vertical"
    MDToolbar:
        title:"K.B.H.S"
        left_action_items:[["book"]]
        elevation:8
    ScrollView:
        do_scroll_x: False
        bar_width:2
        always_overscroll:False

        BoxLayout:
            id:box_layout_1
            orientation:"vertical"
            size_hint_y:None
            size_hint_x:1
            height:self.minimum_height
            spacing:dp(20)
            padding:(dp(0),dp(0),dp(0),dp(60))

            FloatLayout:
                size_hint:1,None
                height:dp(200)
                id:b1
                RelativeLayout:
                    id:box_layout_2_first
                    size:b1.size[0]*.75,b1.size[1]
                    pos:b1.center_x-b1.size[0]*.375,b1.center_y-b1.size[1]/1.5
                    canvas.before:
                        Color:
                            rgba: (0,180/255,216/255,1)
                        RoundedRectangle:
                            size:box_layout_2_first.size[0]*.75,box_layout_2_first.size[1]

                    Button:
                        text:"view"
                        background_color:44/255,51/255,51/255
                        size_hint:None,None
                        size:box_layout_2_first.size[0]*.75,box_layout_2_first.size[1]*.25
                        pos_hint:{"left":1,"bottom":1}
                        on_release:root.change_screen()
                    Label:
                        id:label
                        text:"Administrator"
                        height:self.texture_size[1]
                        font_size:dp(20)
                        pos_hint:{"center_x":.4,"center_y":.5}
                    Image:
                        source:"images/working.png"
                        size_hint:None,None
                        size:box_layout_2_first.size[0]*.75,box_layout_2_first.size[1]*.25
                        pos_hint:{"center_x":.4,"top":1}
                        allow_stretch:True
            FloatLayout:
                size_hint:1,None
                height:dp(200)
                id:b2
                RelativeLayout:
                    id:box_layout_3_first
                    size:b2.size[0]*.75,b2.size[1]
                    pos:b2.center_x-b2.size[0]*.375,b2.center_y-b2.size[1]/1.5
                    canvas.before:
                        Color:
                            rgba: (0,180/255,216/255,1)
                        RoundedRectangle:
                            size:box_layout_3_first.size[0]*.75,box_layout_3_first.size[1]

                    Button:
                        text:"view"
                        background_color:44/255,51/255,51/255
                        size_hint:None,None
                        size:box_layout_3_first.size[0]*.75,box_layout_3_first.size[1]*.25
                        pos_hint:{"left":1,"bottom":1}
                    Label:
                        id:label
                        text:"Teacher"
                        height:self.texture_size[1]
                        font_size:dp(20)
                        pos_hint:{"center_x":.4,"center_y":.5}
                    Image:
                        source:"images/teacher.png"
                        size_hint:None,None
                        size:box_layout_3_first.size[0]*.75,box_layout_3_first.size[1]*.25
                        pos_hint:{"center_x":.4,"top":1}
