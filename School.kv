<SignUpPage>:
    orientation:"vertical"
    MDToolbar:
        title:"SignUp"
        left_action_items:[["arrow-left",lambda x:root.prev_screen()]]
        right_action_items:[["account"]]
        elevation:8
    ScrollView:
        always_overscroll:False
        do_scroll_x:False
        BoxLayout:
            orientation:"vertical"
            size_hint:1,None
            height:self.minimum_height
            spacing:dp(10)

            FloatLayout:
                id:float_layout_5
                size_hint:1,None
                height:dp(100)
                RelativeLayout:
                    id:relative_layout_5_two

                    size:float_layout_5.size
                    pos:float_layout_5.pos
                    Image:
                        source:"images/add-friend.png"
                        size_hint:None,None
                        size:dp(100),dp(100)
                        pos_hint:{"center_x":.5,"top":1}
            FloatLayout:
                id:float_layout_6
                size_hint:1,None
                height:dp(100)
                RelativeLayout:
                    id:relative_layout_6_two
                    size_hint:None,None
                    size:float_layout_6.size[0]*.75,float_layout_6.size[1]
                    pos:float_layout_6.center_x-float_layout_6.size[0]*.375,float_layout_6.center_y-float_layout_6.size[1]*.5
                    MDTextField:
                        id:text_input_sign_up
                        size_hint:1,.6
                        hint_text:"Enter Username"
                        multiline:False
                        pos_hint:{"center_x":.5,"center_y":.5}
                        padding_y:dp(20),dp(20)
                        required:True
                        helper_text_mode:"on_error"


            FloatLayout:
                id:float_layout_7
                size_hint:1,None
                height:dp(100)
                RelativeLayout:
                    id:relative_layout_7_two

                    size_hint:None,None
                    size:float_layout_7.size[0]*.75,float_layout_7.size[1]
                    pos:float_layout_7.center_x-float_layout_7.size[0]*.375,float_layout_7.center_y-float_layout_7.size[1]*.5
                    MDBoxLayout:
                        MDLabel:
                            text:"+254"
                            pos_hint:{"x":0,"center_y":.5}
                            size_hint:(None,None)
                            width:dp(40)
                            theme_text_color:"Hint"
                        MDTextField:
                            id:text_input_phone_number
                            hint_text:"Enter Phone Number"
                            size_hint:1,.6
                            multiline:False
                            pos_hint:{"center_x":.5,"center_y":.5}
                            max_text_length:9
                            required:True
                            helper_text_mode:"on_error"
            FloatLayout:
                id:float_layout_8
                size_hint:1,None
                height:dp(100)
                RelativeLayout:
                    id:relative_layout_8_two

                    size_hint:None,None
                    size:float_layout_8.size[0]*.75,float_layout_8.size[1]
                    pos:float_layout_8.center_x-float_layout_8.size[0]*.375,float_layout_8.center_y-float_layout_8.size[1]*.5
                    MDTextField:
                        id:text_input_email_sign_up
                        hint_text:"Enter Email"
                        icon_left:"email"
                        size_hint:1,.6
                        multiline:False
                        padding_y:dp(20),dp(20)
                        pos_hint:{"center_x":.5,"center_y":.5}
                        required:True
                        helper_text_mode:"on_error"
            FloatLayout:
                id:float_layout_9
                size_hint:1,None
                height:dp(100)
                RelativeLayout:
                    id:relative_layout_9_two
                    size_hint:None,None
                    size:float_layout_9.size[0]*.75,float_layout_9.size[1]
                    pos:float_layout_9.center_x-float_layout_9.size[0]*.375,float_layout_9.center_y-float_layout_9.size[1]*.5
                    MDBoxLayout:
                        MDTextField:
                            id:text_input_password_sign_up
                            hint_text:"Enter Password"
                            size_hint:1,.6
                            pos_hint:{"center_x":.5,"center_y":.5}
                            multiline:False
                            icon_left:"key-variant"
                            password:True
                            required:True
                            helper_text_mode:"on_error"
                        MDIconButton:
                            id:md_icon_3
                            icon:"eye-off"
                            pos_hint:{"x":1,"center_y":.5}
                            on_release:root.toggle_visibility()
            FloatLayout:
                id:float_layout_10
                size_hint:1,None
                height:dp(100)
                RelativeLayout:
                    id:relative_layout_10_two
                    size_hint:None,None
                    size:float_layout_10.size[0]*.75,float_layout_10.size[1]
                    pos:float_layout_10.center_x-float_layout_10.size[0]*.375,float_layout_10.center_y-float_layout_10.size[1]*.5
                    MDBoxLayout:
                        MDTextField:
                            id:text_input_confirm_password_sign_up
                            hint_text:"Confirm Password"
                            size_hint:1,.6
                            pos_hint:{"center_x":.5,"center_y":.5}
                            multiline:False
                            icon_left:"key-variant"
                            password:True
                            required:True
                            helper_text_mode:"on_error"
                        MDIconButton:
                            id:md_icon_2
                            icon:"eye-off"
                            pos_hint:{"x":1,"center_y":.5}
                            on_release:root.toggle_visibility_confirm()
            FloatLayout:
                id:float_layout_11
                size_hint:1,None
                height:dp(100)
                RelativeLayout:
                    id:relative_layout_11
                    size_hint:None,None
                    size:float_layout_11.size[0]*.75,float_layout_11.size[1]
                    pos:float_layout_11.center_x-float_layout_11.size[0]*.375,float_layout_11.center_y-float_layout_11.size[1]*.5

                    Button:
                        id:button_create_account
                        size_hint:1,.6
                        pos_hint:{"center_x":.5,"center_y":.5}
                        canvas.after:
                            Color:
                                rgba:(0,180/255,216/255,1)
                            Line:
                                rectangle:button_create_account.pos[0],button_create_account.pos[1],button_create_account.size[0],button_create_account.size[1]
                        text:"Create Account"
                        color:1,1,1,1
                        background_color:(0,180/255,216/255,1)
                        background_normal:"1,0,0,1"
                        on_release:root.get_details()
            FloatLayout:
                id:float_layout_12
                size_hint:1,None
                height:dp(100)
                RelativeLayout:
                    id:relative_layout_12
                    size_hint:None,None
                    size:float_layout_12.size[0]*.75,float_layout_12.size[1]
                    pos:float_layout_12.center_x-float_layout_12.size[0]*.375,float_layout_12.center_y-float_layout_12.size[1]*.5

                    Label:
                        id:relative_layout_12_label
                        text:"Already have an account?"
                        color:0,0,0,1
                        height:self.texture_size[1]
                        size_hint:.5,.3
                        pos_hint:{"left":1,"center_y":.5}
                    MDTextButton:
                        text:"login here"
                        size_hint:.5,.3
                        pos_hint:{"right":1.2,"center_y":.5}
                        color:0,0,1
                        background_color:0,0,0,0
                        on_release:root.change_to_login_screen()
